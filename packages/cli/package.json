{
  "name": "@genfeedai/cli",
  "version": "0.1.0",
  "description": "CLI tool to download and manage Genfeed workflow templates",
  "license": "AGPL-3.0",
  "bin": {
    "genfeed": "./dist/bin/genfeed.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@genfeedai/workflows": "workspace:*",
    "commander": "12.1.0",
    "ora": "8.1.1",
    "picocolors": "1.1.1",
    "prompts": "2.4.2"
  },
  "devDependencies": {
    "@types/node": "22.14.1",
    "@types/prompts": "2.4.9",
    "tsup": "8.4.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "genfeed",
    "workflow",
    "cli",
    "templates",
    "ai",
    "video",
    "image"
  ]
}
