{
  "dependencies": {
    "@genfeedai/types": "workspace:*"
  },
  "description": "Core utilities for Genfeed workflow engine",
  "devDependencies": {
    "@vitest/coverage-v8": "3.0.4",
    "tsup": "8.4.0",
    "typescript": "5.9.3",
    "vitest": "3.0.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./topological-sort": {
      "import": "./dist/topological-sort.js",
      "require": "./dist/topological-sort.cjs",
      "types": "./dist/topological-sort.d.ts"
    },
    "./validation": {
      "import": "./dist/validation.js",
      "require": "./dist/validation.cjs",
      "types": "./dist/validation.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "AGPL-3.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "name": "@genfeedai/core",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "git+https://github.com/genfeedai/core.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "bun run build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "version": "0.1.1"
}
